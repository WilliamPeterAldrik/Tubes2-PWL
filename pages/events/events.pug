// header section start
.header_section
  .header_main
    .mobile_menu
      nav.navbar.navbar-expand-lg.navbar-light.bg-light
        .logo_mobile
          a(href='/dashboardAdmin')
            h1.banner_taital Eventify
        button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarNav.collapse.navbar-collapse
          ul.navbar-nav
            li.nav-item
              a.nav-link(href='/dashboardAdmin') Home
            li.nav-item
              a.nav-link(href='/keuangan') Keuangan
            li.nav-item
              a.nav-link(href='/kegiatan') Kegiatan
            li.nav-item
              a.nav-link(href='/events') Events

    .container-fluid
      .logo
        a(href='/dashboardAdmin')
          h1.banner_taital Eventify
      .menu_main
        ul
          li.active
            a(href='/dashboardAdmin') Home
          li
            a(href='/keuangan') Keuangan
          li
            a(href='/kegiatan') Kegiatan
          li
            a(href='/events') Events
// header section end
// services section start
.services_section.layout_padding
  .container
    h1.services_taital Events 
    p.services_text There are Many Event that we have
    .services_section_2
      .row
        .col-md-4
          div
            img.services_img(src='../images/img-1.png')
          .btn_main
            a(href='#') Seminar AI
        .col-md-4
          div
            img.services_img(src='../images/img-2.png')
          .btn_main.active
            a(href='#') GameHoods
        .col-md-4
          div
            img.services_img(src='../images/img-3.png')
          .btn_main
            a(href='#') App Dev
// services section end
// footer section start
.footer_section.layout_padding
  .container
    .input_btn_main
      input.mail_text(type='text' placeholder='Enter your email' name='Enter your email')
      .subscribe_bt
        a(href='#') Subscribe
    .location_main
      .call_text
        img(src='../images/call-icon.png')
      .call_text
        a(href='#') Call +01 1234567890
      .call_text
        img(src='../images/mail-icon.png')
      .call_text
        a(href='#') demo@gmail.com
    .social_icon
      ul
        li
          a(href='#')
            img(src='../images/fb-icon.png')
        li
          a(href='#')
            img(src='../images/twitter-icon.png')
        li
          a(href='#')
            img(src='../images/linkedin-icon.png')
        li
          a(href='#')
            img(src='../images/instagram-icon.png')
